package com.site.sectionIntros{	import flash.display.*;	import flash.events.*;		import com.greensock.*	import com.greensock.easing.*		import com.davidcaneso.framework.DevelopmentEnvironment	import com.davidcaneso.framework.SectionIntro;	import com.davidcaneso.events.framework.DevelopmentEnvironmentEvent	import com.davidcaneso.events.framework.SiteManagerEvent;	import com.davidcaneso.events.framework.IntroEvent;			public class SectionIntro0 extends SectionIntro	{		public function SectionIntro0()		{			// constructor code			_className 			= 'SectionIntro0'			_devSectionNumber	= 0			_devConfigFile		= 'media/xml/siteConfig.xml'			_showTraces 		= false;			_showTraces 		= true;		}				//	private and protected methods		override protected function setup():void		{			classTrace('setup')			TweenMax.to(squareAnimation, 0, {tint:0xff0000})		}				protected override function animateIn():void		{			classTrace('animateIn')			TweenMax.to(squareAnimation, 1, {x:300, y:300, rotation:270, alpha:1, onComplete:animateInComplete})		}				private function animateInComplete():void		{			super.introAnimateInComplete()			TweenMax.delayedCall(1, animateOut)		}				protected override function animateOut():void		{			classTrace('hit animateOut')						TweenMax.killTweensOf(squareAnimation)			TweenMax.to(squareAnimation, 1, {y:500, rotation:0, alpha:0, onComplete:animateOutComplete})		}				private function animateOutComplete():void		{			destroy()			super.introAnimateOutComplete()		}				protected override function sleep():void		{			classTrace('hit sleep');		}				protected override function wake():void		{			classTrace('hit wake');		}				public override function destroy():void		{			classTrace('hit destroy')			super.destroy()		}			}	}