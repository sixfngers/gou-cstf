package com.site{	import flash.text.Font	import flash.events.Event	import com.davidcaneso.utils.StringUtils	import com.davidcaneso.text.EmbededFonts		public class Typefaces extends EmbededFonts	{		private var _fontList:Array;				public function Typefaces() 		{			_fontList = [];						var exportedFonts:Array = Font.enumerateFonts();			var iLimit:int = exportedFonts.length;			for(var i:int = 0; i < iLimit; i++)			{				_fontList.push(StringUtils.removeSpaces(exportedFonts[i].fontName));			}						super.fontsToRegister = _fontList		}				override protected function _init(e:Event):Array		{			var fonts:Array = super._init(e);			trace('\n\n'+'available fonts for use:\n'+fonts[0]+'\n'+'invalid fonts:\n'+fonts[1]+'\n\n');			return fonts;		}			}	}