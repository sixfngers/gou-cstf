package com.site.ui.events{	import flash.events.Event;	import com.site.sections.ProjectData;		public class ProjectThumbEvent extends Event	{		public static const SELECT	:String = 'ProjectThumbSelect';				private var _data:ProjectData;				public function ProjectThumbEvent(type:String, projectData:ProjectData, bubbles:Boolean = false, cancelable:Boolean = false) 		{ 			_data = projectData;			super(type, bubbles, cancelable);		}				public function get data():ProjectData		{			return _data;		}				public override function clone():Event 		{ 			return new ProjectThumbEvent(type, _data, bubbles, cancelable);		} 				public override function toString():String 		{ 			return formatToString('ProjectThumbEvent', 'type', '_data', 'bubbles', 'cancelable', 'eventPhase');		}			}}