package com.site.ui.events{	import flash.events.Event;		public class FullDisplayProjectChangeEvent extends Event	{		public static const CHANGE	:String = 'FullDisplayProjectChange';				private var _projectNumber:int;		private var _direction:int;				public function FullDisplayProjectChangeEvent(type:String, projectNumber:int, direction:int, bubbles:Boolean = false, cancelable:Boolean = false) 		{ 			_projectNumber = projectNumber;			_direction = direction			super(type, bubbles, cancelable);		}				public function get projectNumber():int		{			return _projectNumber;		}				public function get direction():int		{			return _direction;		}				public override function clone():Event 		{ 			return new FullDisplayProjectChangeEvent(type, _projectNumber, direction, bubbles, cancelable);		} 				public override function toString():String 		{ 			return formatToString('FullDisplayProjectChangeEvent', 'type', '_projectNumber', 'direction', 'bubbles', 'cancelable', 'eventPhase');		}			}}