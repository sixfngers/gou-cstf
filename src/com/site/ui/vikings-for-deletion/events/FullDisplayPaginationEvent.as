package com.site.ui.events{	import flash.events.Event;	import com.site.sections.ProjectData;		public class FullDisplayPaginationEvent extends Event	{		public static const CHANGE	:String = 'FullDisplayPaginationChange';				private var _buttonNumber:int;		private var _direction:int;				public function FullDisplayPaginationEvent(type:String, buttonNumber:int, direction:int, bubbles:Boolean = false, cancelable:Boolean = false) 		{ 			_buttonNumber = buttonNumber;			_direction = direction;			super(type, bubbles, cancelable);		}				public function get buttonNumber():int		{			return _buttonNumber;		}				public function get direction():int		{			return _direction;		}				public override function clone():Event 		{ 			return new FullDisplayPaginationEvent(type, _buttonNumber, _direction, bubbles, cancelable);		} 				public override function toString():String 		{ 			return formatToString('FullDisplayPaginationEvent', 'type', '_buttonNumber', '_direction', 'bubbles', 'cancelable', 'eventPhase');		}			}}