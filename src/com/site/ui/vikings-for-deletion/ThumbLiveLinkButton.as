package com.site.ui{	import flash.display.*;	import flash.text.*;	import flash.events.*;	import flash.geom.Point;		import com.greensock.*	import com.greensock.easing.*		import com.davidcaneso.framework.Section;	import com.davidcaneso.framework.DevelopmentEnvironment	import com.davidcaneso.singletons.RuntimeAssets	import com.davidcaneso.singletons.Styling	import com.davidcaneso.singletons.XMLData	import com.davidcaneso.events.framework.DevelopmentEnvironmentEvent	import com.davidcaneso.events.framework.SiteManagerEvent;	import com.davidcaneso.events.framework.SectionEvent;	import com.davidcaneso.text.DynamicTextField;	import com.davidcaneso.framework.SimpleLink;	import com.davidcaneso.display.drawing.SquareArea;	import com.davidcaneso.display.buttons.BasicButton	import com.davidcaneso.display.buttons.LinkButton				public class ThumbLiveLinkButton extends LinkButton	{		private var _container:Sprite		private var _icon:Bitmap		private var _fill:SquareArea		private var _area:SquareArea				public function ThumbLiveLinkButton(href:String, fillColor:Number)		{			super(href);			_icon = RuntimeAssets.instance.duplicateAsset('thumbLiveLinkIcon');			_icon.x = -_icon.width			_icon.cacheAsBitmap = true;						_fill = new SquareArea(_icon.width, _icon.height, fillColor)			_fill.x = -15;			_fill.cacheAsBitmap = true;						_fill.mask = _icon;						_area = new SquareArea(_icon.width, _icon.height)			_area.x = _icon.x			_area.alpha = 0;						_container = new Sprite()			_container.addChild(_area)			_container.addChild(_fill)			_container.addChild(_icon)						addChild(_container);		}				public function enableButton():void		{			super.enable()		}				public function disableButton():void		{			super.disable()		}				override protected function buttonRollOver():void		{			TweenMax.to(_fill, .3, {x:-_fill.width, ease:Quad.easeOut})		}				override protected function buttonRollOut():void		{			TweenMax.to(_fill, .3, {x:-15, ease:Quad.easeOut})		}			}	}