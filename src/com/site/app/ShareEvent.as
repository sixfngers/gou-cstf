package com.site.app{    import flash.events.Event;    public class ShareEvent extends Event    {        public static const SHARE    :String = 'share';        public static const FACEBOOK:String = 'facebook';        public static const GOOGLE:String   = 'google';        public static const TUMBLR:String   = 'tumblr';        public static const TWITTER:String  = 'twitter';        public static const PINTREST:String = 'pintrest';        private var _shareType:String;        public function ShareEvent(type:String,bubbles:Boolean=false,cancelable:Boolean=false)        {            super(type,bubbles,cancelable);        }        public function set shareType(val:String):void        {            _shareType = val;        }        public function get shareType():String        {            return _shareType;        }        public override function clone():Event        {			var clonedShareEvent:ShareEvent = new ShareEvent(type, bubbles,cancelable);				clonedShareEvent.shareType = _shareType;							return clonedShareEvent;        }        public override function toString():String        {            return formatToString('ShareEvent','type','_shareType','bubbles','cancelable','eventPhase');        }    }}