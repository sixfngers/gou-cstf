package com.site{	import com.davidcaneso.utils.LiveTrace;	import com.site.Shell;	import flash.display.*;	import flash.events.*;	import com.greensock.*	import com.greensock.easing.*	import com.davidcaneso.framework.SiteManager	import com.davidcaneso.framework.ActiveSiteElement;	import com.davidcaneso.framework.DevelopmentEnvironment	import com.davidcaneso.events.framework.SiteManagerEvent;	import com.davidcaneso.singletons.StageReference;	import com.davidcaneso.display.drawing.SquareArea;	//[SWF(backgroundColor=0x000000, frameRate=30, width=1038, height=700)]	public class Header extends ActiveSiteElement	{        private var _stageContent:MovieClip;        		public function Header()		{			_className 			= 'Header';			_devConfigFile 		= '../../assets/data/siteConfig.xml';			_devSetupType		= DevelopmentEnvironment.INITIAL_EVENT_TYPES;			_devSectionNumber 	= -1;			_showTraces 		= false;						//	uncomment the _showTraces var on the line below to activate traces for this section			_showTraces = true;		}				override public function destroy():void		{			super.destroy()		}				//	private and protected methods		override protected function setup():void		{            _stageContent = new MovieClip;            addChild(_stageContent);						var _fpoBg = new SquareArea(100, 30, 0xffffff);			_fpoBg.x = -(_fpoBg.width);			_stageContent.addChild(_fpoBg);			classTrace('_top = '+_top);			//_top.addEventListener(TableauEvent.HIDE_CURRENT_CHARACTER_NAV, _handleShowVideoEvent);						handleResize();			stage.addEventListener(Event.RESIZE, handleResize);		}		override protected function handleSiteStateUpdate(e:SiteManagerEvent):void		{			switch(e.siteState){				case SiteManager.INITIAL_ANIMATION_STATE:					animateIn();					break;				case SiteManager.SECTION_TRANS_IN_STATE:					break;				case SiteManager.SECTION_TRANS_OUT_STATE:					break;					case SiteManager.SLEEP_STATE:					sleep();					break;									case SiteManager.WAKE_STATE:					wake();					break;			}		}						private function animateIn():void		{			classTrace('hit animateIn');		}				private function animateInComplete():void		{			classTrace('animateInComplete');			//stage.dispatchEvent(new SiteManagerEvent(SiteManagerEvent.INITIAL_ANIMATION_COMPLETE, _top.siteState, _top.activeSectionNumber, _top.nextSectionNumber))		}				protected override function sleep():void		{			classTrace('hit sleep');					}				protected override function wake():void		{			classTrace('hit wake');					}				private function handleResize(e:Event = null):void		{			//			var s:Stage = StageReference.instance.stage;			var w:int = s.stageWidth;            _stageContent.x = w;		}	}	}