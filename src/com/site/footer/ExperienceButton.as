package com.site.footer{		import flash.display.MovieClip;	import com.davidcaneso.display.buttons.BasicButton	import flash.events.Event;	public class ExperienceButton extends BasicButton	{						public function ExperienceButton()		{			// constructor code			super()		}				public function enableButton():void		{			super.enable()		}				public function disableButton():void		{			super.disable()		}				override protected function buttonRollOver():void		{			this.gotoAndStop(2);		}				override protected function buttonRollOut():void		{			this.gotoAndStop(1);		}				override protected function buttonClick():void		{			dispatchEvent(new Event(Event.SELECT));		}	}	}