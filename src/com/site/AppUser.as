package com.site{    import flash.errors.IllegalOperationError;    public class AppUser	{		private static var _instance:AppUser;        private static var _dataModel:AppUserDataModel;        private static var _imageSequence:AppUserVideoImageSequence;//        private static var _userVideoPath:String;//        private static var _userGifPath:String;//        private static var _userName:String;//        private static var _userOrigin:String;		public static function get instance():AppUser		{			if (!AppUser._instance)            {                AppUser._instance = new AppUser(new PrivateClass());                AppUser._dataModel = new AppUserDataModel();                AppUser._imageSequence = new AppUserVideoImageSequence();            }			return AppUser._instance;		}		public function AppUser(pvt:PrivateClass):void		{			if (pvt == null)			{				throw new IllegalOperationError("You may not instantiate this Singleton class directly. Use the static getInstance() method.");			}			else			{			}		}        public static function resetUser():void        {            _dataModel.reset();        }        public static function get dataModel():AppUserDataModel        {            return _dataModel;        }        public static function get imageSequence():AppUserVideoImageSequence        {            return _imageSequence;        }        public static function get base_path():String        {            return _dataModel.basePath;        }        public static function get userVideoPath():String        {            return _dataModel.userVideoPath;        }        public static function set userVideoPath(value:String):void        {            _dataModel.userVideoPath = value;        }        public static function get userImagePath():String        {            return _dataModel.userImagePath;        }        public static function set userImagePath(value:String):void        {            _dataModel.userImagePath = value;        }        public static function get userGifPath():String        {            return _dataModel.userGifPath;        }        public static function set userGifPath(value:String):void        {            _dataModel.userGifPath = value;        }        public static function get userName():String        {            return _dataModel.userName;        }        public static function set userName(value:String):void        {            _dataModel.userName = value;        }        public static function get userId():String        {            return _dataModel.id;        }        public static function set userId(value:String):void        {            _dataModel.id = value;        }        public static function get userOrigin():String        {            return _dataModel.userOrigin;        }        public static function set userOrigin(value:String):void        {            _dataModel.userOrigin = value;        }    }}//Singleton Enforcerclass PrivateClass { }