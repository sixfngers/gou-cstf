package com.davidcaneso.utils{	public class MathUtils	{		//constructor		public function MathUtils():void		{		}		//methods		public static function randomWithinRange(min:Number,max:Number):Number		{			return min + Math.random() * (max - min);		}		public static function roundNumber(numberToRound:Number,decimalPlaces:int=0):Number		{			var factor:Number = Math.pow(10,decimalPlaces);			return int((numberToRound * factor)) / factor;		}		public static function degreesToRadians(degrees:Number):Number		{			return degrees * Math.PI / 180;		}		public static function radiansToDegrees(radians:Number):Number		{			return radians * (180 / Math.PI);		}	}}