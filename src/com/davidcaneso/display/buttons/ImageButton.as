package com.davidcaneso.display.buttons{		//	imports	import flash.display.DisplayObject;		public class ImageButton extends BasicButton	{		//	properties		private var _image:DisplayObject;				//	constructor		public function ImageButton(image:DisplayObject, isEnabledAtCreation:Boolean = true):void		{			super(isEnabledAtCreation);			_image = image;			addChild(_image);		}				// getters & setters		public function get image():DisplayObject		{			return DisplayObject(_image);		}			}		}