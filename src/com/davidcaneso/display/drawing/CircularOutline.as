package com.davidcaneso.display.drawing{	import flash.display.BlendMode;	import flash.display.Sprite;		public class CircularOutline extends Sprite	{		private var _outer	:CircularArea;		private var _inner	:CircularArea;				public function CircularOutline(outerRadius:Number, innerRadius:Number, alignCenter:Boolean = true, outerColor:Number = 0x000000, innerColor:Number = 0x000000):void		{			//var _center = new SquareArea(10, 10, 0x000000, true)			var outerPosx:Number = 0;			var outerPosy:Number = 0;			var innerPosx:Number = 0;			var innerPosy:Number = 0;						if(!alignCenter)			{				innerPosx = outerRadius - innerRadius;				innerPosy = outerRadius - innerRadius;			}						_outer = new CircularArea(outerRadius, outerColor, alignCenter);			_outer.x = outerPosx;			_outer.y = outerPosy;						_inner = new CircularArea(innerRadius, innerColor, alignCenter);			_inner.x = innerPosx;			_inner.y = innerPosy;			addChild(_outer);			addChild(_inner);		}				public function knockout():void		{			this.blendMode = BlendMode.LAYER;			_inner.blendMode = BlendMode.ERASE;		}		//getters and setters		public function get inner():CircularArea		{			return _inner;		}		public function get outer():CircularArea		{			return _outer;		}	}}