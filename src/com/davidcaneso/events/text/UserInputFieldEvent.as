package com.davidcaneso.events.text{	import flash.events.Event;	import com.davidcaneso.text.UserInputField;	public class UserInputFieldEvent extends Event	{		public static const FOCUS_CHANGE:String = 'UserInputFieldFocus';		public static const VALUE_CHANGE:String = 'UserInputFieldValueChange';		private var _field:UserInputField;		public function UserInputFieldEvent(type:String,field:UserInputField,bubbles:Boolean=false,cancelable:Boolean=false):void		{			_field = field;			super(type,bubbles,cancelable);		}				public function get field():UserInputField		{			return _field		}		public override function clone():Event		{			return new UserInputFieldEvent(type,_field,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('UserInputFieldEvent','type','field','bubbles','cancelable','eventPhase');		}	}}