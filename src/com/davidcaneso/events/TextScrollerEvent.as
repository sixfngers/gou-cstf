package com.davidcaneso.events{	import flash.events.Event;	public class TextScrollerEvent extends Event	{		public static const UPDATE:String = 'textScrollerUpdate';		public static const DEACTIVATE:String = 'textScrollerDeactivate';		public static const ACTIVATE:String = 'textScrollerActivate';		private var _percent:int;		public function TextScrollerEvent(type:String,sliderPercent:int=0,bubbles:Boolean=false,cancelable:Boolean=false):void		{			_percent = sliderPercent;			super(type,bubbles,cancelable);		}		public function get percent():int		{			return _percent;		}		public override function clone():Event		{			return new SliderEvent(type,_percent,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('SliderEvent','type','_percent','bubbles','cancelable','eventPhase');		}	}}