package com.davidcaneso.events.video{	import flash.events.Event;	public class VideoPlaybackEvent extends Event	{		public static const METADATA_RECEIVED:String = 'videoMetaDataReceived';		public static const PLAYER_STATE_CHANGE:String = 'videoPlayerStateChange';		public static const PLAYER_TIME_CHANGE:String = 'videoPlayerTimeChange';        public static const INVALID_FORMAT:String = 'videoPlayerInvalidFormat';        public static const FILE_NOT_FOUND:String = 'videoPlayerFileNotFound';		private var _playerState:String;		private var _percentPlayed:int;		private var _metaData:Object;		public function VideoPlaybackEvent(type:String,playerState:String,percentPlayed:int,metaData:Object=null,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			_playerState = playerState;			_percentPlayed = percentPlayed;			_metaData = metaData;		}		public function get playerState():String		{			return _playerState;		}		public function get percentPlayed():int		{			return _percentPlayed;		}		public function get metaData():Object		{			return _metaData;		}		public override function clone():Event		{			return new VideoPlaybackEvent(type,playerState,percentPlayed,metaData,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('VideoPlaybackEvent','type','playerState','percentPlayed','metaData','bubbles','cancelable','eventPhase');		}	}}