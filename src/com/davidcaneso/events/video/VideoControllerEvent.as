package com.davidcaneso.events.video{	import flash.events.Event;	public class VideoControllerEvent extends Event	{		public static const PAUSE:String = 'VideoControlPause';		public static const SCRUB_START:String = 'VideoControlScrubStart';		public static const SCRUB_UPDATE:String = 'VideoControlScrubUpdate';		public static const SCRUB_END:String = 'VideoControlScrubEnd';		public static const MUTE:String = 'VideoControlMute';		public static const UNMUTE:String = 'VideoControlUnmute';		public static const FULL_SCREEN_OPEN:String = 'VideoControlFullScreenOpen';		public static const FULL_SCREEN_CLOSE:String = 'VideoControlFullScreenClose';		private var _scrubPercent:int;		public function VideoControllerEvent(type:String,scrubPercent:int=-1,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			_scrubPercent = scrubPercent;		}		public function get scrubPercent():int		{			return _scrubPercent;		}		public override function clone():Event		{			return new VideoControllerEvent(type,scrubPercent,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('VideoControllerEvent','type','scrubPercent','bubbles','cancelable','eventPhase');		}	}}