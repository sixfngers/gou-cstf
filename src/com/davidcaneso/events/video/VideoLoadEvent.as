package com.davidcaneso.events.video{	import flash.events.Event;	public class VideoLoadEvent extends Event	{		public static const UPDATE:String = 'videoLoadUpdate';		private var _percent:int;		public function VideoLoadEvent(type:String,percent:int,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			_percent = percent;		}		public function get percent():int		{			return _percent;		}		public override function clone():Event		{			return new VideoLoadEvent(type,percent,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('VideoLoadEvent','percent','metaData','bubbles','cancelable','eventPhase');		}	}}