package com.davidcaneso.events.loading{	import flash.events.Event;	public class CssLoaderEvent extends Event	{		public static const START:String = 'cssLoadStart';		public static const COMPLETE:String = 'cssLoadComplete';		public static const ERROR:String = 'cssLoadError';		public var cssText:String;		public function CssLoaderEvent(type:String,p_cssText:String='',bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			cssText = p_cssText;		}		public override function clone():Event		{			return new CssLoaderEvent(type,cssText,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('CssLoaderEvent','type','cssText','bubbles','cancelable','eventPhase');		}	}}