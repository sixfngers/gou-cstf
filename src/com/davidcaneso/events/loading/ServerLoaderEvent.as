package com.davidcaneso.events.loading{	import flash.events.Event;	public class ServerLoaderEvent extends Event	{		public static const START:String = 'serverLoadStart';		public static const COMPLETE:String = 'serverLoadComplete';		public static const ERROR:String = 'serverLoadError';		public var loadedData:*;		public function ServerLoaderEvent(type:String,p_data: * =null,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			loadedData = p_data;		}		public override function clone():Event		{			return new ServerLoaderEvent(type,loadedData,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('ServerLoaderEvent','type','loadedData','bubbles','cancelable','eventPhase');		}	}}