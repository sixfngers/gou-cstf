package com.davidcaneso.events.loading{	import flash.events.Event;	public class FileUploaderEvent extends Event	{		public static const BROWSE_START:String = 'fileBrowse';		public static const BROWSE_CANCEL:String = 'fileBrowseCancel';		public static const SELECT:String = 'fileSelect';		public static const START:String = 'loadStart';		public static const UPDATE:String = 'loadUpdate';		public static const COMPLETE:String = 'loadComplete';		public static const ERROR:String = 'loadError';		private var _file:String;		private var _percent:int;		public function FileUploaderEvent(type:String,p_file:String,p_percent:int,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			_file = p_file;			_percent = p_percent;		}		public function get file():String		{			return _file;		}		public function get percent():int		{			return _percent;		}		public override function clone():Event		{			return new FileUploaderEvent(type,file,percent,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('FileUploaderEvent','type','file','percent','bubbles','cancelable','eventPhase');		}	}}