package com.davidcaneso.events.navigation{	import flash.events.Event;	public class NavigationEvent extends Event	{		public static const CHANGE:String = 'activeNavElementChange';		public var previouslyActiveNavNumber:int;		public var activeNavNumber:int;		public function NavigationEvent(type:String,p_previouslyActiveNavNumber:int,p_activeNavNumber:int,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			previouslyActiveNavNumber = p_previouslyActiveNavNumber;			activeNavNumber = p_activeNavNumber;		}		public override function clone():Event		{			return new NavigationEvent(type,previouslyActiveNavNumber,activeNavNumber,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('NavigationEvent','previouslyActiveNavNumber','activeNavNumber','type','bubbles','cancelable','eventPhase');		}	}}