package com.davidcaneso.events.framework{	import flash.events.Event;	public class IntroEvent extends Event	{		public static const TRANSITION_IN_START:String = 'introTransitionInStart';		public static const TRANSITION_IN_COMPLETE:String = 'introTransitionInComplete';		public static const TRANSITION_OUT_START:String = 'introTransitionOutStart';		public static const TRANSITION_OUT_COMPLETE:String = 'introTransitionOutComplete';		public static const TRANSITION_OUT_TO_LOADER_COMPLETE:String = 'introTransitionOutToLoaderComplete';		public function IntroEvent(type:String,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);		}		public override function clone():Event		{			return new IntroEvent(type,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('IntroEvent','type','bubbles','cancelable','eventPhase');		}	}}/*public static const INITIAL_LOADING_STATE:String= 'initialLoadState';public static const INITIAL_ANIMATION_STATE:String= 'initialAnimationState';public static const INITIAL_ANIMATION_COMPLETE_EVENT:String= 'initialAnimationCompleteEvent';public static const INITIAL_ANIMATION_COMPLETE_STATE:String= 'initialAnimationCompleteState';public static const SECTION_LOADING_STATE:String= 'sectionLoadingState';public static const SECTION_ACTIVE_STATE:String= 'sectionActiveState';public static const SECTION_TRANS_IN_STATE:String= 'sectionTransInState';public static const SECTION_TRANS_OUT_STATE:String= 'sectionTransOutState';public static const SECTION_TRANS_IN_COMPLETE_STATE:String = 'sectionTransInCompleteState';public static const SECTION_TRANS_OUT_COMPLETE_STATE:String= 'sectionTransOutCompleteState';public static const INTRO_LOADING_STATE:String= 'introLoadingState';public static const INTRO_ACTIVE_STATE:String= 'introActiveState';public static const INTRO_TRANS_IN_STATE:String= 'introTransInState';public static const INTRO_TRANS_OUT_EVENT:String= 