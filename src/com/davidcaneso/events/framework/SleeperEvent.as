package com.davidcaneso.events.framework{	import flash.events.Event;	public class SleeperEvent extends Event	{		public static const SLEEPER_ACTIVATED:String = 'sleeperActivated';		public static const SLEEPER_DEACTIVATED:String = 'sleeperDeactivated';		public function SleeperEvent(type:String,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);		}		public override function clone():Event		{			return new SleeperEvent(type,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('SleeperEvent','type','bubbles','cancelable','eventPhase');		}	}}