package com.davidcaneso.events.framework{	import flash.events.Event;	public class SiteManagerEvent extends Event	{		public static const SITE_STATE_UPDATE:String = 'siteStateUpdate';		public static const STOP_CURRENT_LOAD:String = 'stopCurrentLoad';		public static const INITIAL_ANIMATION_START:String = 'initialAnimationStart';		public static const INITIAL_ANIMATION_COMPLETE:String = 'initialAnimationComplete';		public static const INTRO_TRANSITION_OUT:String = 'introAnimateOut';		public static const INTRO_TRANSITION_OUT_TO_LOADER:String = 'introAnimateOutToLoader';		public static const SECTION_TRANSITION_OUT:String = 'sectionAnimateOut';		public var siteState:String;		public var currentSectionNumber:int;		public var nextSectionNumber:int;		public function SiteManagerEvent(type:String,p_siteState:String,p_currentSectionNumber:int,p_nextSectionNumber:int,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			siteState = p_siteState;			currentSectionNumber = p_currentSectionNumber;			nextSectionNumber = p_nextSectionNumber;		}		public override function clone():Event		{			return new SiteManagerEvent(type,siteState,currentSectionNumber,nextSectionNumber,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('SiteManagerEvent','type','siteState','currentSectionNumber','nextSectionNumber','bubbles','cancelable','eventPhase');		}	}}/*public static const INITIAL_LOADING_STATE:String= 'initialLoadState';public static const INITIAL_ANIMATION_STATE:String= 'initialAnimationState';public static const INITIAL_ANIMATION_COMPLETE_EVENT:String= 'initialAnimationCompleteEvent';public static const INITIAL_ANIMATION_COMPLETE_STATE:String= 'initialAnimationCompleteState';public static const SECTION_LOADING_STATE:String= 'sectionLoadingState';public static const SECTION_ACTIVE_STATE:String= 'sectionActiveState';public static const SECTION_TRANS_IN_STATE:String= 'sec