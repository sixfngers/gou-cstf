package com.davidcaneso.events.framework{	import flash.events.Event;	public class SectionIntroEvent extends Event	{		//public static const TRANSITION_IN_START:String = 'sectionTransitionInStart';		public static const TRANSITION_IN_COMPLETE:String = 'introTransitionInComplete';		//public static const TRANSITION_OUT_START:String = 'sectionTransitionOutStart';		public static const TRANSITION_OUT_COMPLETE:String = 'introTransitionOutComplete';		public static const TRANSITION_OUT_TO_LOADER_COMPLETE:String = 'introTransitionOutToLoaderComplete';		public function SectionIntroEvent(type:String,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);		}		public override function clone():Event		{			return new SectionIntroEvent(type,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('SectionIntroEvent','type','bubbles','cancelable','eventPhase');		}	}}