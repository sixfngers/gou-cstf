package com.davidcaneso.events.framework{	import flash.events.Event;	import flash.display.MovieClip;	public class TypefaceSetupEvent extends Event	{		public static const SETUP:String = 'typefacesSetup';		public var mc:MovieClip;		public function TypefaceSetupEvent(type:String,p_mc:MovieClip,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			mc = p_mc;		}		public override function clone():Event		{			return new TypefaceSetupEvent(type,mc,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('TypefaceSetupEvent','type','mc','bubbles','cancelable','eventPhase');		}	}}