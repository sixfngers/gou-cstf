package com.davidcaneso.events.buttonEvent{	import flash.events.Event;	public class ToggleButtonEvent extends Event	{		public static const UPDATE:String = 'ToggleUpdate';		public var isOn:Boolean;		public function ToggleButtonEvent(type:String,p_isOn:Boolean,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			isOn = p_isOn;		}		public override function clone():Event		{			return new ToggleButtonEvent(type,isOn,bubbles,cancelable);		}		public override function toString():String		{			return formatToString('ToggleButtonEvent','isOn','type','bubbles','cancelable','eventPhase');		}	}}