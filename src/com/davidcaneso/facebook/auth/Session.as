package com.davidcaneso.facebook.auth{	import com.davidcaneso.utils.LiveTrace	public class Session	{		private var _session			:Object;		private var _base_domain		:String = '';		private var _access_token		:String = '';		private var _secret				:String = '';		private var _expires			:String = '';		private var _sig				:String = '';		private var _session_key		:String = '';		private var _uid				:String = '';						public function Session(pSession:Object)		{			/*			var s:String;			s = "Facebook Session - Object Enumeration:\n************************************\n";						for (var property:String in pSession)			{				s += "- " + property + ": " + pSession[property] + "\n";			}						s += "************************************\n";						LiveTrace.output(s);			*/			_session 		= pSession;			_base_domain 	= _session.base_domain;			_access_token 	= _session.access_token;			_secret 		= _session.secret;			_expires 		= _session.expires;			_sig 			= _session.sig;			_session_key 	= _session.session_key;			_uid 			= _session.uid;						LiveTrace.output('Session Created '+this.toString());		}				public function endSession():void		{			_session 		= null;			_base_domain 	= '';			_access_token 	= '';			_secret 		= '';			_expires		= '';			_sig 			= '';			_session_key 	= '';			_uid 			= '';		}				public function toString():String		{			var s:String;			s = "Facebook Session Object Enumeration:\n************************************\n";						for (var property:String in _session)			{				s += "- " + property + ": " + _session[property] + "\n";			}						s += "************************************\n";						return s;		}				//Getters		//*******		public function get base_domain()	:String { return _base_domain; 	}		public function get access_token()	:String { return _access_token; }		public function get secret()		:String { return _secret;		}		public function get expires()		:String { return _expires; 		}		public function get sig()			:String { return _sig; 			}		public function get session_key()	:String { return _session_key;	}		public function get uid()			:String { return _uid; 			}	}}