package com.davidcaneso.framework {import com.davidcaneso.singletons.StageReference;import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.Event;import flash.external.ExternalInterface;public class InactiveSiteElement extends MovieClip	{		//properties		protected var _top:MovieClip;		protected var _xml:XMLList;		//development properties		protected var _dev:Boolean;		protected var _className:String = 'defaultClassName';		protected var _showTraces:Boolean;		//constructor		public function InactiveSiteElement()		{			_dev = false;			_showTraces = true;			if ((stage != null))			{				_dev = true;			}			addEventListener(Event.ADDED_TO_STAGE,handleAddedToStage);		}		private function handleAddedToStage(e:Event):void		{			removeEventListener(Event.ADDED_TO_STAGE,handleAddedToStage);			if (! _dev)			{                _top = SiteManager(StageReference.instance.stage.getChildAt(0));			}			init();		}		protected function init():void		{		}		public function destroy():void		{            classTrace('destroy', _className);			removeEventListener(Event.ADDED_TO_STAGE,handleAddedToStage);		}		protected function classTrace(val: * ,className:String=''):void		{			if (! _showTraces)			{				return;			}			var prefix:String = '';			if (className.length > 0)			{				prefix = className + ': ';			}			else if (_className.length > 0)			{				prefix = _className + ': ';			}			trace((prefix + val));            if(ExternalInterface.available)                ExternalInterface.call('console.log', (prefix + val));		}	}}