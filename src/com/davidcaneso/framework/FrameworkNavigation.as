package com.davidcaneso.framework {	import com.davidcaneso.singletons.StageReference;	import com.davidcaneso.singletons.XMLData;	import com.davidcaneso.utils.LiveTrace;	import flash.events.Event;	// imports	public class FrameworkNavigation extends ActiveSiteElement	{		//properties		//constructor		public function FrameworkNavigation():void		{			trace('Navigation constructor');		}		public function changeSections(nextSectionNumber:int):void		{			LiveTrace.output('changeSections '+nextSectionNumber+' '+SiteManager(_top).useSWFAddress)			if (_top.useSWFAddress)			{				LiveTrace.output('next swf address at sectionNumber = '+nextSectionNumber+' '+XMLData.instance.sitemap.sectionlist.section.@deeplink[nextSectionNumber]);				_top.updateSWFAddress(XMLData.instance.sitemap.sectionlist.section.@deeplink[nextSectionNumber]);			}			else				_top.prepareNextSection(nextSectionNumber);		}		public function finalizeSetup():void		{//			these last two calls should not proceed any other call//			make sure the dev buttons are on top			if(_dev)				addChild(_top);//			only for 100% browser sites and full screen//			leave in place as part of the generic framework			_handleResize();			StageReference.instance.stage.addEventListener(Event.RESIZE, _handleResize);		}		protected function _handleResize(e:Event = null):void		{		}	}}