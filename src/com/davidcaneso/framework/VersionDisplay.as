package com.davidcaneso.framework{	import flash.display.Sprite;	import flash.ui.ContextMenu;	import flash.ui.ContextMenuBuiltInItems;	import flash.ui.ContextMenuItem;	public class VersionDisplay extends Sprite	{		public function VersionDisplay(versionValue:String)		{			var menuLabel:String = versionValue;			var myContextMenu:ContextMenu = new ContextMenu  ;			myContextMenu.hideBuiltInItems();			var defaultItems:ContextMenuBuiltInItems = myContextMenu.builtInItems;			defaultItems.print = true;			var item:ContextMenuItem = new ContextMenuItem(menuLabel);			myContextMenu.customItems.push(item);			this.contextMenu = myContextMenu;		}	}}