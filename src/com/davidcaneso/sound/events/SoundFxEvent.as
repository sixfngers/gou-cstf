package com.davidcaneso.sound.events{	import flash.events.Event;	public class SoundFxEvent extends Event	{		public static const SOUND:String = "playSoundEffect";		public var soundName:String;		public var soundVolume:Number;		public function SoundFxEvent(type:String,p_soundName:String,p_soundVolume:Number=-1,bubbles:Boolean=false,cancelable:Boolean=false)		{			super(type,bubbles,cancelable);			soundName = p_soundName;			soundVolume = p_soundVolume;		}		public override function clone():Event		{			return new SoundFxEvent(type,soundName,soundVolume,bubbles,cancelable);		}		public override function toString():String		{			return formatToString("SoundFxEvent","type","soundName",'soundVolume',"bubbles","cancelable","eventPhase");		}	}}