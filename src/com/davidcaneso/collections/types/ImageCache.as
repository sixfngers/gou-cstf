package com.davidcaneso.collections.types{			import com.davidcaneso.collections.List;	import flash.display.BitmapData;	public class ImageCache extends List	{		public function ImageCache():void		{			super();		}				public function addImage(imageUrl:String, imageData:BitmapData):void		{			super.addToList(imageUrl, imageData);		}				public function removeImage(imageUrl:String):BitmapData		{			return BitmapData( super.removeFromList(imageUrl) );		}				public function findByName(imageUrl:String):BitmapData		{			return BitmapData( super.findInList(imageUrl) );		}				public function destroy():void		{			var allImages:Array = super.names;			var limit:int = allImages.length;						for(var i:int = 0; i < limit; i++){				BitmapData(findByName(allImages[0])).dispose();				super.removeFromList(allImages[0]);			}		}			}	}